<div class="wrap-view-image">
  <div class="d-flex justify-content-between">
    <div class="ml-4 mt-2">
      <div class="btn btn-primary">General</div>
    </div>
    <div class="mr-4 mt-2">
      <button class="btn-close-modal btn btn-danger">Close</button>
    </div>
  </div>
  <br />
  <br />
  <br />

  <form class="d-flex justify-content-center" method="post" enctype="multipart/form-data">
    <input class="form-control input-form-control input-upload"   name="file" type="file" multiple />
  </form>
  <div class="d-flex justify-content-center">
    <a type-image="general"  class=" btn btn-success mt-2 btn-upload-image" >Upload</a>
  </div>
  <br />
  <br />
  <br />
  <div class="d-flex">
    <a class="btn btn-success ml-2 btn-submit">Submit</a>
  </div>
  <div class="d-flex flex-wrap ml-2 mr-2" id="list-image">
   <% if (typeof arrayImage !== 'undefined' && arrayImage.length > 0) { %>
        <% arrayImage.forEach(function(item) { %>
        <div   class=" m-1 wrap-image">
            <img  src="<%=item.url%>"  class="image-upload"/>
            <input id-item="<%=item.id%>" name="image-check" url="<%=item.url%>" type="radio" id="ip-checkbox" />
        </div>
        <% }); %>
    <% } else {}%>
</div>
</div>

